# add additional provide to AWS cmd line config
aws configure --profile new_profile_name
aws sts get-caller-identity --profile new_profile_name

# get k8s config
aws eks update-kubeconfig --region $REGION --name $EKS_NAME

# get vpcs
aws ec2 describe-vpcs --region ca-cental-1
kubectl config view
kubectl config current-context
